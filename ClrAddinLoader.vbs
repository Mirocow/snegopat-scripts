pathToDll = Replace(SelfScript.fullPath,".vbs",".dll")
set fso = CreateObject("Scripting.FileSystemObject")
If Not (fso.FileExists(pathToDll)) Then
  CreateDll pathToDll
End If

LoaderRegistered = True
On Error Resume Next
set clrloader = CreateObject("SnegopatLib.ClrAddinLoader")
If Err.Number <> 0 Then 'не зарегистрирован :(
    LoaderRegistered = False
    Err.Clear
End If
On Error Goto 0
If Not LoaderRegistered Then
    Set WshShell = CreateObject("WScript.Shell")
    Set windir = fso.GetSpecialFolder(0)
    Set folder = fso.GetFolder(windir & "\Microsoft.NET\Framework\")
    Set folders = folder.SubFolders
    highestver = ""
    FrameworkDir = ""
    For each folderIdx In folders
        If Left(folderIdx.Name,1)="v" Then
            ver = Mid(folderIdx.Name,2,3)
            If highestver<ver Then
                HighestVer = Ver
                FrameworkDir = folderIdx.Path
            End If
        End if
    Next
    If FrameWorkDir<>"" Then
        regFilePath = fso.GetSpecialFolder(2).path&"\ClrAddinLoader.reg"
        WshShell.Run FrameWorkDir & "\RegAsm.exe " & pathToDll & " /codebase /regfile:"&regFilePath,0,True 
        Set objFile = fso.OpenTextFile(regFilePath, 1)
        strText = objFile.ReadAll
        objFile.Close
        strNewText = Replace(strText, "HKEY_CLASSES_ROOT", "HKEY_CURRENT_USER\Software\Classes")
        Set objFile = fso.OpenTextFile(regFilePath, 2)
        objFile.WriteLine strNewText
        objFile.Close
'        WshShell.Run windir & "\regedit.exe /s " & regFilePath,0,True 
'        не получилось :( все равно требует Elevation и не вносит CLSID
        Set objTextFile = fso.OpenTextFile(regFilePath, 1)
        currBranch = ""
'stop
        Do Until objTextFile.AtEndOfStream
          strNextLine = objTextFile.Readline
          If Left(strNextLine,1)="[" Then
            currBranch = Mid(strNextLine,2,Len(strNextLine)-2)
          End If
          dpos = InStr(strNextLine,"=")
          If dpos>0 Then
            key = Replace(left(strNextLine,dpos-1),"""","")
            val = Replace(mid(strNextLine,dpos+1),"""","")
            if key = "@" Then
              WshShell.RegWrite currBranch&"\",val
            Else
              WshShell.RegWrite currBranch&"\"&key,val
            End If
          End If
        Loop
        WshShell.RegWrite currBranch&"\",""
        objTextFile.Close
    End If
    set clrloader = CreateObject("SnegopatLib.ClrAddinLoader")
End If
addins.registerLoader(clrloader)
